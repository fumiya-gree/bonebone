<h2>トレーニング一覧</h2>
<span>選んだコース以外の筋トレも取り入れてみよう！</span>
  <% @contents.each do |content| %>
  <ul class="contents_menu">
    <div class="thumbnail__caption">
      <h3 class="text-xsmall text-overflow" title="コース名">
        <%= content.course_name %>
      </h3>
      <li>
        <%= link_to "#{content.menu_name_1}", "/contents/#{content.menu_name_1}" %>
      </li>
      <li>
        <%= link_to "#{content.menu_name_2}", "/contents/#{content.menu_name_2}" %>
      </li>
      <li>
        <%= link_to "#{content.menu_name_3}", "/contents/#{content.menu_name_3}" %>
      </li>
    </div>
  </ul>
  <% end %>
